{
	"manifest_version": 2,
	"name": "Pocket Search",
	"description": "Adds pocket search capabilities to omnibox.",
	"version": "1.0",
	"author": "IODuck Software",
	"background": {
		"scripts": [
			"api.js",
			"background.js"
		]
	},
	"omnibox": {
		"keyword": "pocket"
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"https://getpocket.com/"
	],
	"web_accessible_resources": [
		"authorized.html"
	]
}
